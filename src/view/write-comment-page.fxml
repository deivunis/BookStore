<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="245.0" prefWidth="395.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.WriteCommentPage">
    <children>
        <TextArea fx:id="commentText" layoutX="14.0" layoutY="60.0" prefHeight="131.0" prefWidth="367.0" promptText="Comment text.." />
        <Button layoutX="14.0" layoutY="202.0" mnemonicParsing="false" onAction="#saveComment" prefHeight="25.0" prefWidth="99.0" text="Save" />
        <Button fx:id="cancel" layoutX="282.0" layoutY="202.0" mnemonicParsing="false" onAction="#returnFromComment" prefHeight="25.0" prefWidth="99.0" text="Cancel" />
      <TextField fx:id="commentTitle" layoutX="14.0" layoutY="20.0" prefHeight="25.0" prefWidth="367.0" promptText="Title.." />
    </children>
</AnchorPane>
